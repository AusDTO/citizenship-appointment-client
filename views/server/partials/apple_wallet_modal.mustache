<div id="add-to-wallet-modal" class="modal-bg">
    <div class="modal-div" role="dialog" aria-label="Instructions for adding a pass to Apple Wallet.">
        <div class="modal-div-content">
            <a href="#close-modal" title="Close" aria-label="Close this dialog" class="close" tabindex="2"><img src="/images/times.svg" onerror="this.src='/images/times.png';" class="icon" alt="" role="presentation"/></a>
            <div class="add-to-wallet-modal-content">
                {{> add_to_wallet_instructions }}
            </div>
        </div>
    </div>
</div>
