<td class="DateCell Calendar-date--bookable {{^available_times_count}}Calendar-date--unavailable{{/available_times_count}} {{^bookable}}Calendar-date--hide{{/bookable}} date-{{date}}">
  <div class="DateCell-content">

    <a class="DateCell-content--datelink date-{{date}}" href="#date/{{ date }}" name="date/{{ date }}">
      {{#today}}
        <span class="DateCell-content--tag">TODAY</span>
      {{/today}}
      <p class="DateCell-content--day">{{ day }}</p>
      {{#available_times_count}}<p class="DateCell-content--availableTimesCount">{{available_times_count}} available</p>{{/available_times_count}}
    </a>
  </div>
</td>
